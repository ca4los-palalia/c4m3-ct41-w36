										<comboitem
											label="@load(each.concater)"
											tooltiptext="@load(each.descripcion)" />
									</template>
								</combobox>
								
								<intbox width="100%" value="@bind(vm.capturador.miercoles.hrs)" maxlength="2"  placeholder="hrs trabajadas" id="intMiercoles" disabled="@bind(vm.capturador.miercoles.vmDisabled)" onOK="@command('agregarRegistro')"/>
