									selectedItem="@bind(vm.capturador.viernes.incidencia)"
									model="@load(vm.incidenciasCatalogo)">
									<template name="model">
										<comboitem
											label="@load(each.concater)"
											tooltiptext="@load(each.descripcion)" />
									</template>
								</combobox>
								
								<intbox width="100%" value="@bind(vm.capturador.sabado.hrs)" maxlength="2"  placeholder="hrs trabajadas" id="intSabado" disabled="@bind(vm.capturador.sabado.vmDisabled)" onOK="@command('agregarRegistro')"/>
								<combobox width="100%" disabled="@bind(vm.capturador.sabado.vmDisabled)"
									selectedItem="@bind(vm.capturador.sabado.incidencia)"
									model="@load(vm.incidenciasCatalogo)">
									<template name="model">
										<comboitem
											label="@load(each.concater)"
											tooltiptext="@load(each.descripcion)" />
