		<borderlayout style="background: #0F0F0F;">
			<north size="220px" sclass="panelTransparent">
				<vlayout>
					<grid sclass="my-grid panelTransparent" width="100%">
						<auxhead>
							<auxheader colspan="1" sclass="listboxBuscadorHeader">
								<vlayout>
									<button label="Ver horario" width="95%" sclass="btnClear btnAddBorder btnFormatLabel btnViewer" onClick="@command('verHorario')" id="btnVerHorario" visible="false"/>
									<grid sclass="my-grid panelTransparent" width="100%">
										<columns>
											<column width="60px"/>
											<column/>
										</columns>
										<rows>
											<row>
												<label sclass="titleFormat formatLabelOnFormulario" value="Fecha Registro:" />
												<datebox width="100%" value="@bind(vm.capturador.fechaRegistro)" readonly="true" format="yyyy-MM-dd HH:mm:ss"/>
											</row>
										</rows>
									</grid>
								</vlayout>
							</auxheader>
							<auxheader colspan="2" sclass="listboxBuscadorHeader">
								<label
									sclass="titleFormat formatLabelOnFormulario" value="@bind(vm.capturador.lunes.tagDay)" />
							</auxheader>
							<auxheader colspan="2" sclass="listboxBuscadorHeader">
								<label
									sclass="titleFormat formatLabelOnFormulario" value="@bind(vm.capturador.martes.tagDay)" />
							</auxheader>
							<auxheader colspan="2" sclass="listboxBuscadorHeader">
								<label
									sclass="titleFormat formatLabelOnFormulario" value="@bind(vm.capturador.miercoles.tagDay)" />
							</auxheader>
							<auxheader colspan="2" sclass="listboxBuscadorHeader">
								<label
									sclass="titleFormat formatLabelOnFormulario" value="@bind(vm.capturador.jueves.tagDay)" />
							</auxheader>
							<auxheader colspan="2" sclass="listboxBuscadorHeader">
								<label
									sclass="titleFormat formatLabelOnFormulario" value="@bind(vm.capturador.viernes.tagDay)"/>
							</auxheader>
							<auxheader colspan="2" sclass="listboxBuscadorHeader">
								<label
									sclass="titleFormat formatLabelOnFormulario" value="@bind(vm.capturador.sabado.tagDay)" />
							</auxheader>
							<auxheader colspan="2" sclass="listboxBuscadorHeader">
								<label
									sclass="titleFormat formatLabelOnFormulario" value="@bind(vm.capturador.domingo.tagDay)" />
							</auxheader>
						</auxhead>
						<columns>
							<column sclass="listboxBuscadorHeader" width="270px">
								<label
									sclass="titleFormat formatLabelOnFormulario" value="Colaborador" />
							</column>
							
							<column sclass="listboxBuscadorHeader">
