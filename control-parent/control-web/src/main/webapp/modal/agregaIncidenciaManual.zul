											tooltiptext="@load(each.descripcion)" />
									</template>
								</combobox>
								
								<intbox width="100%" value="@bind(vm.capturador.jueves.hrs)" maxlength="2"  placeholder="hrs trabajadas" id="intJueves" disabled="@bind(vm.capturador.jueves.vmDisabled)" onOK="@command('agregarRegistro')"/>
