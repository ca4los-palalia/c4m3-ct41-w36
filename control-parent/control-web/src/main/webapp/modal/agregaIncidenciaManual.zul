										<column />
										<column width="35px" align="left"/>
									</columns>
									<rows>
										<row> 
											<textbox width="100%" value="@bind(vm.capturador.usuario.datosGenerales.persona.nombreCompleto)" readonly="true"/>			
											<button image="/images/toolbar/searchNew16.png" width="100%" sclass="btnClear" tooltiptext="Buscar colaborador" onClick="@command('clickedBuscador')" id="btnFindUsr"/>
										</row>
									</rows>
								</grid>
								
								
								<intbox width="100%" value="@bind(vm.capturador.lunes.hrs)" maxlength="2"  placeholder="hrs trabajadas" id="intLunes" disabled="@bind(vm.capturador.lunes.vmDisabled)" onOK="@command('agregarRegistro')"/>
								<combobox width="100%" disabled="@bind(vm.capturador.lunes.vmDisabled)"
									selectedItem="@bind(vm.capturador.lunes.incidencia)"
									model="@load(vm.incidenciasCatalogo)">
									<template name="model">
										<comboitem
											label="@load(each.concater)"
											tooltiptext="@load(each.descripcion)" />
									</template>
								</combobox>
								
								<intbox width="100%" value="@bind(vm.capturador.martes.hrs)" maxlength="2"  placeholder="hrs trabajadas" id="intMartes" disabled="@bind(vm.capturador.martes.vmDisabled)" onOK="@command('agregarRegistro')"/>
								<combobox width="100%" disabled="@bind(vm.capturador.martes.vmDisabled)"
									selectedItem="@bind(vm.capturador.martes.incidencia)"
									model="@load(vm.incidenciasCatalogo)">
