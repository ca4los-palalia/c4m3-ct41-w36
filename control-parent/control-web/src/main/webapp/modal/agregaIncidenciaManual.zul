					<grid sclass="my-grid">
						<columns>
							<column width="270px"/>
							<column/>
							<column/>
							<column/>
							<column/>
							<column/>
							<column/>
							<column/>
						</columns>
						<rows>
							<row>
								<grid sclass="my-grid">
									<columns>
										<column/>
										<column/>
									</columns>
									<rows>
										<row>
											<button width="100%" height="100%"
												label="Nuevo"
												sclass="btnClear btnAddBorder btnFormatLabel btnNuevo"
												onClick="@command('nuevoRegistro')" />
												
											<button width="100%" height="100%"
												label="Agregar"
												onClick="@command('agregarRegistro')"
												sclass="btnClear btnAddBorder btnFormatLabel btnAceptar"/>			
										</row>
									</rows>
								</grid>
								
								<textbox width="100%" value="@bind(vm.capturador.lunes.observacion)" placeholder="observaciones" disabled="@bind(vm.capturador.lunes.vmDisabled)" onOK="@command('agregarRegistro')"/>
								<textbox width="100%" value="@bind(vm.capturador.martes.observacion)" placeholder="observaciones" disabled="@bind(vm.capturador.martes.vmDisabled)" onOK="@command('agregarRegistro')"/>
								<textbox width="100%" value="@bind(vm.capturador.miercoles.observacion)" placeholder="observaciones" disabled="@bind(vm.capturador.miercoles.vmDisabled)" onOK="@command('agregarRegistro')"/>
								<textbox width="100%" value="@bind(vm.capturador.jueves.observacion)" placeholder="observaciones" disabled="@bind(vm.capturador.jueves.vmDisabled)" onOK="@command('agregarRegistro')"/>
								<textbox width="100%" value="@bind(vm.capturador.viernes.observacion)" placeholder="observaciones" disabled="@bind(vm.capturador.viernes.vmDisabled)" onOK="@command('agregarRegistro')"/>
								<textbox width="100%" value="@bind(vm.capturador.sabado.observacion)" placeholder="observaciones" disabled="@bind(vm.capturador.sabado.vmDisabled)" onOK="@command('agregarRegistro')"/>
								<textbox width="100%" value="@bind(vm.capturador.domingo.observacion)" placeholder="observaciones" disabled="@bind(vm.capturador.domingo.vmDisabled)" onOK="@command('agregarRegistro')"/>
							</row>
						</rows>
					</grid>
